@model List<UAAASSCM.Controllers.AllScmRequests>
@{
    ViewBag.Title = "Get All SCM details";
    //Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    int count = 1;
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


<table id="example" class="display" border="1">
    <thead style="background-color:#E8EFF3">
        <tr>
            <th style="width: 80px; text-align: center;">
                S.No
            </th>
           
            <th style="width: 220px; text-align: center;">
                College Name
            </th>
            <th style="width: 120px; text-align: center;">
                Department
            </th>
            <th style="width: 120px; text-align: center;">
                Specialization
            </th>
           



        </tr>
    </thead>
    <tbody style="background-color:#FCF1D1">
        @if (Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td style="width: 50px; text-align: center;">
                        @(count++)
                    </td>
                    @*<td style="width: 50px; text-align: left; text-transform: uppercase;">
                            @item.auditorId
                        </td>*@
                    <td style="width: 220px; text-align: left; text-transform: uppercase;">
                        @item.CollegeName
                    </td>
                    <td style="width: 120px; text-align: center;">
                        @item.deptName
                    </td>
                    <td style="width: 120px; text-align: center;">
                        @item.SpecializationName
                    </td>





                </tr>
            }
        }
    </tbody>
   </table>


<style>
    tbody tr td {
    border:1px solid white;
    }
</style>